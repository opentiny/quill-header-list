{
  "name": "quill-header-list",
  "type": "module",
  "version": "0.0.2",
  "packageManager": "pnpm@10.12.1",
  "description": "Automatically generate directory",
  "author": "zzxming",
  "license": "MIT",
  "keywords": [
    "header",
    "quill",
    "directory"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.css": "./dist/index.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint . --cache",
    "build": "tsx ./scripts/build.ts",
    "dev": "tsx ./scripts/build.ts watch",
    "server": "tsx ./scripts/start-server.ts"
  },
  "peerDependencies": {
    "quill": "^2.0.3"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.2",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@zzxming/eslint-config": "^0.5.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "gulp": "^5.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^10.0.0",
    "gulp-sass": "^5.1.0",
    "parchment": "^3.0.0",
    "postcss-pxtorem": "^6.1.0",
    "sass": "^1.83.0",
    "tsdown": "^0.12.7",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "~5.7.3",
    "ws": "^8.18.2"
  }
}
